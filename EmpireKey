if game.CoreGui:FindFirstChild('EmpireHub') then
	game.CoreGui:FindFirstChild('EmpireHub'):Destroy()
end

local link = "https://pastebin.com/raw/GUjdiTGz" 
local body = game:service("HttpService"):JSONDecode(game:HttpGet(link))


