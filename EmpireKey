local StarterGui = game:GetService("StarterGui")

ImageButton.MouseButton1Click:Connect(function()
	setclipboard('https://discord.gg/XzFDmeF8BT')
	TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
	request = http_request or request or HttpPost or syn.request
	if request then
		local HttpService = game:GetService("HttpService");
		local lower = string.lower;
		local function join_server(code)
			return request({
				Url = "http://127.0.0.1:6463/rpc?v=1";
				Method = "POST";
				Body = HttpService:JSONEncode({
					cmd = "INVITE_BROWSER",
					args = {
						code = code
					},
					nonce = lower(HttpService:GenerateGUID(false))
				});
				Headers = {
					["Content-Type"] = "application/json",
					["Origin"] = "https://discord.com"
				}
			});
		end;

		join_server('XzFDmeF8BT')
	end
	wait(1)
	TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
end)
TextButton.MouseButton1Click:Connect(function()
	if TextBox.Text ~= body.NewKey and TextBox.Text ~= body.PredKey then
		StarterGui:SetCore("SendNotification", {
			Title = "EmpireHub v.1.0";
			Text = "Invalid Key."
		})
	end
	if TextBox.Text == body.PredKey then
		StarterGui:SetCore("SendNotification", {
			Title = "EmpireHub v.1.0";
			Text = "New Key."
		})
	end
	if string.gsub(TextBox.Text, " ", "") == body.NewKey then
		StarterGui:SetCore("SendNotification", {
			Title = "EmpireHub v.1.0";
			Text = "Loading..."
		})
		a = Instance.new("LocalScript", game:GetService("TestService"))
		a.Name = "Players"
        loadstring(game:HttpGet('https://pastebin.com/raw/mqWJnyd0'))()
	end
end)
